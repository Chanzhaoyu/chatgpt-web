{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "yml",
    "markdown"
  ],
  "cSpell.words": [
    "antfu",
    "axios",
    "bumpp",
    "chatgpt",
    "commitlint",
    "esno",
    "GPTAPI",
    "iconify",
    "nodata",
    "OPENAI",
    "pinia",
    "rushstack",
    "Sider",
    "tailwindcss",
    "tsup",
    "Typecheck",
    "unplugin",
    "VITE",
    "vueuse",
    "Zhao"
  ]
}
